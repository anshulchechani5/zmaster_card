<core:FragmentDefinition xmlns="sap.m"  xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
<Dialog id="pp1" title="Copy Draft Pattern" >
   <f:SimpleForm id="aloop">
           <Label id="chechanianshul" text="Mastrcard No." />
          
           <ComboBox items="{path : '/ChangeQualityF4', length : '100000'}" change="readdraft" selectionChange="readdraft" showSecondaryValues="true" id="zpno" >
                <items>  
                        <core:ListItem id="combox" text="{Zpno}" key="{Zpno}" additionalText="{ProductDescription}" />
                </items>
            </ComboBox>
   </f:SimpleForm>
   
   <endButton>
   <Button id="close" text="Close" press="onclosedialog2" ></Button>
   </endButton>
   </Dialog>
  
</core:FragmentDefinition>
